{
    "compilerOptions": {
      "module": "commonjs",
      "outDir": "dist",
      "target": "es2020",
      "moduleResolution": "node",
      "sourceMap": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noImplicitAny": true,
      "noImplicitReturns": true
    },
    "include": [
      "src/**/*"
, "src/app.ts"    ],
    "exclude": [
      "dist"
    ]
  }